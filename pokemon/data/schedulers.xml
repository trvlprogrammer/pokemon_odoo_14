<odoo>
	<data noupdate="1">
		<record id="cron_master_data_pokemon" forcecreate='True' model="ir.cron">
		      <field name="name">Get Master Data Pokemon</field>
		      <field eval="True" name="active"/>
		      <field name="user_id" ref="base.user_root"/>
		      <field name="interval_number">1</field>
		      <field name="interval_type">months</field>
		      <field name="numbercall">-1</field>
		      <field name="model_id" ref="model_pokemon_pokemon"/>
		      <field name="state">code</field>
		      <field name="code">model.get_master_data_pokemon()</field>
	   </record>	
   </data>
</odoo>